{
    "name": "eksi search",
    "version": "0.0.1",
    "manifest_version": 2,
    "description": "This extension searches selected text in eksisozluk.com",
    "author":"https://github.com/ibrahimyuce07",
    "icons": {
      "16": "assets/favicon-16x16.png",
      "32": "assets/favicon-32x32.png",
      "48": "assets/favicon-48x48.png",
      "128": "assets/favicon-128x128.png"
    },
    "background": {
        "page": "index.html"
    },
    "permissions": [
      "contextMenus", 
      "tabs"],
      "options_ui": {
        "page": "options.html",
        "open_in_tab": false
      }
  }